/dts-v1/;
/ {
	compatible = "rc-module,mpw7705";

	aliases {
		spi0 = &spi0;
	} ; 

	lsif0 {
	    u-boot,dm-pre-reloc;
	    compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
	

	    spi0: spi0@0x3C03E000 {
		    compatible = "rc-module,pl022-spi";
		    #address-cells = <1>;
		    #size-cells = <0>;
		    reg = <0x3C03E000 0x1000>;
		    num-cs = <1>;
		    spi-max-frequency = <12500000>;
		    u-boot,dm-pre-reloc;

		    flash: m25p80@0 {
			    compatible = "st,m25p32", "jedec,spi-nor";
			    reg = <0>;
			    u-boot,dm-pre-reloc;
		    };
	    };
	}; 
};
